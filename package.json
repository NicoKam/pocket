{
  "private": true,
  "name": "orca-pocket",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist -r git@github.com:NicoKam/orca-pocket-page.git",
    "docs:serve": "http-server docs-dist --spa",
    "build-all": "lerna run build ",
    "build": "echo \"ok\"",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "dep": "ts-node -T checkDep.ts ",
    "c": "changeset && changeset version && npm run dep",
    "pub": "npm run test && changeset publish && git push",
    "pub:only": "npm run test && changeset publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "lint-staged": "lint-staged",
    "test": "pnpm --filter=@orca-fe/* run test ",
    "test:coverage": "umi-test --coverage",
    "postinstall": "husky install"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@babel/runtime": "^7.22.5",
    "@changesets/cli": "^2.26.1",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/react-router": "^5.1.20",
    "ahooks": "^3.7.7",
    "antd": "^5.6.1",
    "babel-plugin-import": "^1.13.6",
    "classnames": "^2.3.2",
    "dumi": "^2.2.1",
    "father": "^4.2.3",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "moment": "^2.29.4",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@orca-fe/eslint-config": "^2.3.3",
    "@orca-fe/jss-plugin-auto-prefix": "^0.0.1",
    "@orca-fe/simple-jss": "^0.0.3",
    "@types/file-saver": "^2.0.5",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.17",
    "chalk": "^5.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "file-saver": "^2.0.5",
    "globby": "^13.1.4",
    "inquirer": "^9.2.7",
    "jest": "^29.5.0",
    "jss": "^10.10.0",
    "react-json-view": "^1.21.3",
    "semver": "^7.5.1",
    "ts-node": "^10.9.1"
  }
}