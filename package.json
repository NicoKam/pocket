{
  "private": true,
  "name": "orca-pocket",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist -r git@github.com:NicoKam/orca-pocket-page.git",
    "docs:serve": "http-server docs-dist --spa",
    "build-all": "father-build ",
    "build": "echo \"ok\"",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "pub": "npm run build-all && lerna publish",
    "pub:only": "lerna publish",
    "pub:force": "npm run build-all && lerna publish --force-publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "lint-staged": "lint-staged",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "postinstall": "husky install"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.15",
    "@types/react-router": "^5.1.18",
    "@umijs/test": "^3.5.28",
    "ahooks": "^3.5.2",
    "ahooks-v2": "^2.10.15",
    "antd": "^4.21.6",
    "babel-plugin-import": "^1.13.5",
    "copyfiles": "^2.4.1",
    "detect-browser": "^5.3.0",
    "dumi": "^1.1.45",
    "father-build": "^1.22.3",
    "gh-pages": "^3.2.3",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "immer": "^9.0.16",
    "json5": "^2.2.1",
    "k-tools": "^1.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "mjolnir.js": "^2.7.1",
    "prefix-classnames": "^0.0.6",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "rc-animate": "^3.1.1",
    "react-countup": "^6.3.0",
    "react-transition-group": "^4.4.2",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.76.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@orca-fe/eslint-config": "^2.2.0",
    "eslint": "^8.19.0",
    "react-json-view": "^1.21.3"
  }
}
