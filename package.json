{
  "private": true,
  "name": "orca-pocket",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider dumi dev",
    "docs:build": "cross-env NODE_OPTIONS=--openssl-legacy-provider dumi build",
    "docs:deploy": "gh-pages -d docs-dist -r git@github.com:NicoKam/orca-pocket-page.git",
    "docs:serve": "http-server docs-dist --spa",
    "build-all": "lerna run build ",
    "build": "echo \"ok\"",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "c": "changeset && changeset version",
    "pub": "changeset publish && git push",
    "pub:only": "lerna publish",
    "pub:force": "npm run build-all && lerna publish --force-publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "lint-staged": "lint-staged",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "postinstall": "husky install"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/runtime": "^7.21.0",
    "@changesets/cli": "^2.26.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^16.14.35",
    "@types/react-dom": "16",
    "@types/react-router": "^5.1.19",
    "ahooks": "^3.7.2",
    "antd": "^4.24.1",
    "babel-plugin-import": "^1.13.5",
    "classnames": "^2.3.2",
    "dumi": "^1.1.49",
    "father": "^4.1.3",
    "gh-pages": "^3.2.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "moment": "^2.29.4",
    "prettier": "^2.7.1",
    "react": "16",
    "react-dom": "16",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@orca-fe/eslint-config": "^2.3.3",
    "@orca-fe/jss-plugin-auto-prefix": "^0.0.1",
    "@orca-fe/simple-jss": "^0.0.1",
    "@types/file-saver": "^2.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "file-saver": "^2.0.5",
    "jss": "^10.10.0",
    "react-json-view": "^1.21.3"
  }
}
